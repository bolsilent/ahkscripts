List := "Cor blimey mate what are ye doing in me pockets?", "Yum!", "Ow! I nearly broke a tooth!", "Aaah nothing like a nice cuppa tea!",


StringSplit, element, List, `,



~LButton Up::
Random, RandomNum, 1, %element0%
randomnumber := RandomNumb(0, 999)
Send, % element%RandomNum% 
MsgBox, 64, Random Name, % element%RandomNum% 
Sleep, 3000 



Send, {Enter Down}
Send, {Enter Up}
return

Ran(Num)
{
   Random, r, -1*Num, Num
   Return r
}

RandomNumb(min,max)
{
   Random, r, %min%, %max%
   Return r
}
